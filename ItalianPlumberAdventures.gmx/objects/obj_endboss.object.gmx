            <kind>1</kind>
            <string>///End boss logic

//When visible
if visible = 1
{

    //Execute default event
    event_inherited()

    //If Mario touches it
    if place_meeting(x,y,obj_mario)
    {
        if (instance_number(obj_block_unlockable) == 0){
            //Finishes the current level and return to map
            //Dissapear
            visible = 0
            instance_create(x,y,obj_smoke)
    
            //Boss clear music
            audio_play_sound(snd_bossclear,1,0)
    
            //Disable Mario's controls
            with obj_mario disablecontrols = 1
    
            //Clear the level
            global.clear = 1
    
            //Clear checkpoints
            global.checkpoint = noone
    
            //Stop timer
            obj_levelmanager.alarm[2] = 0
    
            //Start timer countdown
            alarm[1] = 350
        }else{
            //Used in other type of levels
            //Dissapear
            visible = 0
            //Remove all unlockable block in level
            instance_create(x,y,obj_smoke)
            with (obj_block_unlockable){
                audio_play_sound(snd_break,1,0)
                instance_destroy()
            }
        }
    }

}
</string>
